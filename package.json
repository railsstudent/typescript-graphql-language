{
  "name": "typescript-graphql-language",
  "version": "1.0.0",
  "description": "Setup a GraphQL playground in to query Spanish phrases I learnt",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/railsstudent/typescript-graphql-language.git"
  },
  "keywords": [
    "typescript",
    "express",
    "graphql",
    "apollo-server",
    "docker",
    "typeorm",
    "postgreSql"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/railsstudent/typescript-graphql-language/issues"
  },
  "homepage": "https://github.com/railsstudent/typescript-graphql-language#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "rimraf": "^3.0.2",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "apollo-server": "^2.18.2",
    "express": "^4.17.1",
    "graphql": "^15.3.0"
  }
}
